{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "Authentication": {
    "Google": {
      "ClientId": "<GoogleClientId>",
      "ClientSecret": "<GoogleClientSecret>",
      "Authority": "<GoogleAuthority>",
      "Tokenendpoint": "<GoogleTokenEndpoint>",
      "OpenIdConfiguration": "<GoogleOpenIdConfig>"
    },
    "Facebook": {
      "AppId": "<FacebookAppId>",
      "AppSecret": "<FacebookAppSecret>"
    },
    "Jwt": {
      "Key": "<JwtKey>",
      "Issuer": "<JwtIssuer>",
      "Audience": "<JwtAudience>"
    },
    "RefreshToken": {
      "TTLDays": 2
    }
  },
  "FrontendUrl": "<FrontendUrl>",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JobCleanup": {
    "AutoCleanupCompletedJobs": true,
    "RemoveFromDatabase": false,
    "RemoveFromScheduler": true,
    "RetentionPeriod": "00:00:00"
  }
}
